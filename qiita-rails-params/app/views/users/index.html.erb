<h1> 一覧画面 </h1>
<div>
  <h2>検索</h2>
  <%= form_with url: users_path, method: :get do |f| %>
    <%= f.text_field :search_word %>
    <%= f.submit '検索' %>
  <% end %>
</div>

<div>
    <h2>ユーザー一覧</h2>
    <% @users.each do |user| %>
        <div>
            <%= user.id %>人目: <%= link_to user.name, user_path(user.id) %>
        </div>
    <% end %>
</div>